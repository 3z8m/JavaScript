<%- include('../header') %>

<h1 class="mt-3 mb-3"><%= title %></h1>

<table class="table">
    <tr>
        <th>ID</th>
        <td><%= user.id %></td>
    </tr>
    <tr>
        <th>Name</th>
        <td><%= user.name %></td>
